<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="seller-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Webkul_Marketplace::css/product.css"/>
        <css src="jquery/fileUploader/css/jquery.fileupload-ui.css"/>
    </head>
    <body>
        <referenceBlock name="seller.page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Add New Product Label</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="seller.content">
            <block class="Webkul\MarketplaceProductLabels\Block\Label\Labellist" ifconfig="mpproductlabel/general_settings/label_manage" name="mp_add_new_label" template="Webkul_MarketplaceProductLabels::newlabel.phtml" cacheable="false">
                <block class="Webkul\ImageUploader\Block\Gallery" name="mp_edit_labeled" template="Webkul_ImageUploader::gallery.phtml" cacheable="false">
                    <arguments>
                        <argument name="count" xsi:type="number">1</argument>
                        <argument name="presence" xsi:type="number">1</argument>
                        <argument name="view_model" xsi:type="object">Webkul\ImageUploader\ViewModel\TemplateHelper</argument>
                    </arguments>
                </block>
            </block>
        </referenceContainer>
    </body>
</page>
