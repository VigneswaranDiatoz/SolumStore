<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<head>
		<css src="Webkul_MarketplaceProductLabels::css/imageLabel.css"/>
	</head>
	<body>
		<referenceContainer name="content">
			<block class="Webkul\MarketplaceProductLabels\Block\Label\Labellist" name="default_page_label" template="Webkul_MarketplaceProductLabels::defaultLabel.phtml" after="-"/>
		</referenceContainer>

		<referenceContainer name="layout2_seller_account_before_review">
            <block class="Webkul\Marketplace\Block\Account\Navigation" name="seller_label_menu" ifconfig="mpproductlabel/general_settings/label_manage" after="layout2_label_account_navigation" template="Webkul_MarketplaceProductLabels::/layout2/account/navigation.phtml">
				<arguments>
                    <argument name="view_model" xsi:type="object">Webkul\SolumeslAcl\ViewModel\VendorAcl</argument>
                </arguments>
			</block>
        </referenceContainer>
	</body>
</page>
